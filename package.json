{
  "name": "vue-loginservice",
  "version": "0.1.0",
  "main": "./dist/vue-loginservice.common.js",
  "license": "MIT",
  "description": "VueJS plugin for loginservice.io",
  "author": "Philip Callender <philip.callender@tooltwist.com",  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-loginservice ./src/components/index.js",
    "test": "vue-cli-service build",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open",
    "patch-release": "npm version patch && npm publish && git push --follow-tags"
  },
  "keywords": [
    "vue",
    "tooltwist",
    "loginservice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tooltwist/vue-loginservice.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
